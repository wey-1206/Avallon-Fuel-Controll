<%- include('layout/header') %>

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
      <%- include('layout/navbar') %>

        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
              <%- include('layout/topbar') %>

                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <!-- Page Heading -->
                    <div class="d-sm-flex align-items-center justify-content-between mb-4">
                        <h1 class="h3 mb-0 text-gray-800">Dashboard</h1>
                        <a href="#" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm"><i
                                class="fas fa-download fa-sm text-white-50 mr-2"></i>Gerar Relatório</a>
                    </div>

                    <!-- Content Row -->
                    <div class="row">
                        <!-- Teste de Rodagem Card -->
                        <div class="col-xl-3 col-md-6 mb-4 card-collapse-toggle" data-toggle="collapse" data-target="#shared_collapse"
                            role="button" aria-expanded="false" aria-controls="shared_collapse"
                            data-section="teste_de_rodagem">
                            <div class="card border-left-primary shadow h-100 py-2 hover" id="card_teste_rodagem">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div
                                                class="text-xs font-weight-bold text-primary text-uppercase mb-1 d-flex align-items-center">
                                                <i class="bi bi-car-front fs-5 mr-2"></i>
                                                <strong>TESTE DE RODAGEM</strong>
                                            </div>
                                            <div class="h5 mb-0 font-weight-bold text-gray-800">$40,000</div>
                                        </div>
                                        <div class="col-auto">
                                            <i class="bi bi-fuel-pump-fill fa-2x text-gray-300"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Assistência Card -->
                        <div class="col-xl-3 col-md-6 mb-4 card-collapse-toggle" data-toggle="collapse" data-target="#shared_collapse"
                            role="button" aria-expanded="false" aria-controls="shared_collapse"
                            data-section="assistencia">
                            <div class="card border-left-success shadow h-100 py-2 hover" id="card_assistencia">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div
                                                class="text-xs font-weight-bold text-success text-uppercase mb-1 d-flex align-items-center">
                                                <i class="bi bi-wrench fs-5 mr-2"></i>
                                                <strong>ASSISTÊNCIA</strong>
                                            </div>
                                            <div class="h5 mb-0 font-weight-bold text-gray-800">$215,000</div>
                                        </div>
                                        <div class="col-auto">
                                            <i class="bi bi-fuel-pump-fill fa-2x text-gray-300"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Checklist de Entrada Card -->
                        <div class="col-xl-3 col-md-6 mb-4 card-collapse-toggle" data-toggle="collapse" data-target="#shared_collapse"
                            role="button" aria-expanded="false" aria-controls="shared_collapse"
                            data-section="checklist">
                            <div class="card border-left-warning shadow h-100 py-2 hover" id="card_checklist">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div
                                                class="text-xs font-weight-bold text-warning text-uppercase mb-1 d-flex align-items-center">
                                                <i class="bi bi-clipboard2-check-fill fs-5 mr-2"></i>
                                                <strong>CHECKLIST DE ENTRADA</strong>
                                            </div>
                                            <div class="h5 mb-0 font-weight-bold text-gray-800">$215,000</div>
                                        </div>
                                        <div class="col-auto">
                                            <i class="bi bi-fuel-pump-fill fa-2x text-gray-300"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Consumo Card -->
                        <div class="col-xl-3 col-md-6 mb-4 card-collapse-toggle" data-toggle="collapse" data-target="#shared_collapse"
                            role="button" aria-expanded="false" aria-controls="shared_collapse" data-section="consumo">
                            <div class="card border-left-info shadow h-100 py-2 hover" id="card_consumo_atual">
                                <div class="card-body">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                            <div
                                                class="text-xs font-weight-bold text-info text-uppercase mb-1 d-flex align-items-center">
                                                <i class="bi bi-speedometer fs-5 mr-2"></i>
                                                <strong>CONSUMO</strong>
                                            </div>
                                            <div class="row no-gutters align-items-center">
                                                <div class="col-auto">
                                                    <div class="h5 mb-0 mr-3 font-weight-bold text-gray-800">50%</div>
                                                </div>
                                                <div class="col">
                                                    <div class="progress progress-sm mr-2">
                                                        <div class="progress-bar bg-info" role="progressbar"
                                                            style="width: 50%" aria-valuenow="50" aria-valuemin="0"
                                                            aria-valuemax="100"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-auto">
                                            <i class="fas fa-clipboard-list fa-2x text-gray-300"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Único Collapse -->
                        <div class="collapse mt-2 col-xl-12 col-md-12 mb-4" id="shared_collapse">
                            <div class="card card-body shadow" id="collapse_card">
                                <div class="container-fluid">
                                    <div class="d-flex justify-content-between align-items-center">
                                        <h1 class="h3 mb-2 text-gray-800" id="collapse_title">
                                            <i class="fs-2 mr-2" id="collapse_icon"></i> <span
                                                id="collapse_title_text"></span>
                                        </h1>
                                        <button class="btn plus" id="novo_registro">
                                            <i class="bi bi-plus fs-4"></i>
                                        </button>
                                    </div>
                                    <p class="mb-4" id="collapse_description"></p>
                                    <div class="card shadow mb-4">
                                        <div class="card-header py-3">
                                            <h6 class="m-0 font-weight-bold" id="collapse_table_title"></h6>
                                        </div>
                                        <div class="card-body">
                                            <div class="table-responsive">
                                                <table class="table table-bordered" id="dataTable" width="100%"
                                                    cellspacing="0">
                                                    <thead class="">
                                                        <tr>
                                                            <th>Canhoto</th>
                                                            <th>Data</th>
                                                            <th>Placa/OS</th>
                                                            <th>Tipo</th>
                                                            <th>Valor</th>
                                                            <th>Motorista</th>
                                                            <th>NF</th>
                                                            <th>Obs.</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody id="collapse_table_body">
                                                        <!-- Conteúdo da tabela será preenchido dinamicamente -->
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Content Row -->

                    <div class="row" id="indicadores_row">

                        <!-- Area Chart -->
                        <div class="col-xl-8 col-lg-7">
                            <div class="card shadow mb-4">
                                <!-- Card Header - Dropdown -->
                                <div
                                    class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                                    <h6 class="m-0 font-weight-bold text-primary">Review durante o ano</h6>

                                </div>
                                <!-- Card Body -->
                                <div class="card-body">
                                    <div class="chart-area">
                                        <canvas id="myAreaChart"></canvas>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Pie Chart -->
                        <div class="col-xl-4 col-lg-5">
                            <div class="card shadow">
                                <!-- Card Header - Dropdown -->
                                <div
                                    class="card-header py-2 d-flex flex-row align-items-center justify-content-between">
                                    <h6 class="m-0 font-weight-bold text-primary">Uso</h6>
                                    <div class="dropdown no-arrow">
                                        <a class="dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                                            data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                            <i class="fas fa-ellipsis-v fa-sm fa-fw text-gray-400"></i>
                                        </a>
                                        <div class="dropdown-menu dropdown-menu-right shadow animated--fade-in"
                                            aria-labelledby="dropdownMenuLink">
                                            <div class="dropdown-header">Dropdown Header:</div>
                                            <a class="dropdown-item" href="#">Action</a>
                                            <a class="dropdown-item" href="#">Another action</a>
                                            <div class="dropdown-divider"></div>
                                            <a class="dropdown-item" href="#">Something else here</a>
                                        </div>
                                    </div>
                                </div>
                                <!-- Card Body -->
                                <div class="card-body">
                                    <div class="chart-pie pt-4 pb-2">
                                        <canvas id="myPieChart"></canvas>
                                    </div>
                                    <div class="mt-4 text-start small">
                                        <span class="mr-2">
                                            <i class="fas fa-circle text-primary"></i> Teste de Rodagem
                                        </span>
                                        <br>
                                        <span class="mr-2">
                                            <i class="fas fa-circle text-success"></i> Assistência
                                        </span>
                                        <br>
                                        <span class="mr-2">
                                            <i class="fas fa-circle text-warning"></i> Checklist de Entrada
                                        </span>
                                        <br>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                </div>
                <!-- /.container-fluid -->

            </div>


        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

   <div id="toast-container" style="position: fixed; top: 1rem; right: 1rem; z-index: 9999;"></div>

 <%- include('layout/modals') %>


 <%- include('layout/footer') %>
